{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test (win.exe)",
      "type": "cppvsdbg",
      "request": "launch",
      "preLaunchTask": "Build .exe",
      "cwd": "${workspaceFolder}",
      "program": "Build\\QConsole.exe",
      "args": [],
      "stopAtEntry": false,
      "environment": [],
      "externalConsole": true
    },
    {
      "name": "Test (nux.bin)",
      "type": "cppdbg",
      "request": "launch",
      "preLaunchTask": "Build .bin",
      "cwd": "/mnt/d/QentemCpp",
      "program": "Build/QConsole.bin",
      "stopAtEntry": false,
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "pipeTransport": {
        "pipeCwd": "",
        "pipeProgram": "c:/windows/sysnative/bash.exe",
        "pipeArgs": [
          "-c"
        ],
        "debuggerPath": "/usr/bin/gdb"
      },
      "sourceFileMap": {
        "/mnt/d": "D:\\"
      }
    },
    {
      "name": "Test (win.py)",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}\\Source\\Addon\\TestWrapper.py",
      "console": "integratedTerminal"
    }
  ]
}
